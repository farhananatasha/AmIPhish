{
  "manifest_version": 2,
  "name": "AmIPhish",
  "description": "Helps to detect phishing websites.",
  "version": "0.1",
  
  "browser_action": {
    "default_icon": "icon.png"
  },
  
  "background": {
        "scripts": ["background.js"]
   },
  "content_scripts": [
      {
        "matches": [
          "<all_urls>"
        ],
        "js": ["jquery-3.1.1.min.js","content.js"]
      }
   ]
}
